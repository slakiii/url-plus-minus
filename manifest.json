{
  "manifest_version": 3,
  "name": "URL Plus",
  "version": "1.0",
  "description": "Increase the ending number of the current URL",
  "permissions": ["activeTab"],
  "action": {
    "default_title": "URL Plus",
    "default_icon": {
      "16": "icons/arrow-up-16.png",
      "32": "icons/arrow-up-32.png",
      "64": "icons/arrow-up-64.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
